             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:SIGameIRL"
             mc:Ignorable="d" Width="151.788" Height="77.924" HorizontalAlignment="Left" VerticalAlignment="Top" Grid.Column="1">
    <Grid x:Name="gridMain" RenderTransformOrigin="0.5,0.5">
        <Label x:Name="laName" Content="Label" Margin="4,10,4,0" FontWeight="Bold" Background="{x:Null}" Foreground="#FFE8FF00" AutomationProperties.Name="laName" HorizontalContentAlignment="Center" Height="26" VerticalAlignment="Top"/>
